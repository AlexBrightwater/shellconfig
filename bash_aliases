alias keys='ssh-add ~/.ssh/priv-keys/*'
alias key="ssh-add"

alias lsa='exa -aghl --group-directories-first'
alias ll='exa -agh1 --group-directories-first --icons'
alias ls='exa -agh --group-directories-first --icons'
alias l='exa -agh --group-directories-first --icons'
alias lsm='exa --long --header --links  --inode --blocks --accessed --modified --created --group --all --sort=.name  --group-directories-first'
